   "source": [
    "# Euclid's LEmma for finding GCD(a,b)\n",
    "# GCD(a,b) = GCD(b, a mod b)\n",
    "\n",
    "def euclidean_gcd(a: int,b: int) -> int:\n",
    "    while b != 0:\n",
    "        a,b = b, a%b\n",
    "    return abs(a)\n",
    "\n",
    "print(\"GCD(48,180): {}\".format(euclidean_gcd(48,180)))"
   ]

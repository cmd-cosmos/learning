    "def safe_division(a, b):\n",
    "    if b == 0:\n",
    "        return None, None\n",
    "    return a, b\n",
    "\n",
    "res, err = safe_division(100, 0)\n",
    "if err == None:\n",
    "    print(\"CODE\", err,\": division by zero error\")\n",
    "else:\n",
    "    print(res)"

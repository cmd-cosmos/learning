   ],
   "source": [
    "import os\n",
    "\n",
    "r, w = os.pipe()\n",
    "\n",
    "os.write(w, b\"message to aliens\")\n",
    "os.close(w)\n",
    "\n",
    "data = os.read(r, 1024)\n",
    "os.close(r)\n",
    "\n",
    "print(\"pipe received: \", data.decode())"
   ]

   "source": [
    "# rewrite a regular text file with hashed data\n",
    "\n",
    "import hashlib\n",
    "\n",
    "with open(\"text.txt\", \"rb\") as f:\n",
    "    data = f.read()\n",
    "\n",
    "hashed_data = hashlib.sha256(data=data).hexdigest()\n",
    "\n",
    "with open(\"text.txt\", \"w\") as f:\n",
    "    f.write(hashed_data)\n",
    "\n",
    "print(\"file over written with SHA 256 hash\")"
   ]

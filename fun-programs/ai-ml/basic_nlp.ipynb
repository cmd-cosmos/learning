    "import re\n",
    "\n",
    "def tokenize(text):\n",
    "    text = text.lower()\n",
    "    text = re.sub(r\"[^a-z0-9\\s]\", \"\", text)\n",
    "    return text.split()\n",
    "\n",
    "text = \"I am Batman\"\n",
    "tokens = tokenize(text=text)\n",
    "tokens"
   ]
